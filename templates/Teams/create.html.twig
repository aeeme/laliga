{% extends 'base.html.twig' %}
{% block body %}

    <h1>Equipos</h1>
    <h2> Crea un equipo </h2>
    {% if form is defined %}
        {{ form_start(form) }}
        {{ form_row(form.nombre) }}
        {{ form_row(form.presupuestoActual) }}

        <button type="submit" class="btn btn-primary">Crear</button>
        {{ form_end(form) }}
    {% endif %}

{% endblock %}